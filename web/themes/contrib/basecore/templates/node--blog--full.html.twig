{#
/**
 * @file
 * Base+'s theme implementation to display a blog node.
 *
 */
#}
{% extends "node.html.twig" %}

{% block meta_area %}
  {{ title_prefix }}
  {% if not page %}
    <h2{{ title_attributes.addClass('node__title') }}>
      <a href="{{ url }}" rel="bookmark">{{ label }}</a>
    </h2>
  {% endif %}
  {{ title_suffix }}
{% endblock %}
