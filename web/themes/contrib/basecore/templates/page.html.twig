      {% endif %}

      {% if page.sub_footer_first or page.footer %}
        {# subfooter #}
        <div {% if subfooter_id %}id="{{ subfooter_id }}"{% endif %} class="clearfix subfooter {{ subfooter_background_color }} {{ subfooter_separator }} {{ (subfooter_blocks_paddings) ? ' region--no-block-paddings' : '' }}{{ (subfooter_region_paddings) ? ' region--no-paddings' : '' }}">
