        <div {% if subfooter_top_id %}id="{{ subfooter_top_id }}"{% endif %} class="clearfix subfooter-top {{ subfooter_top_background_color }} {{ subfooter_top_separator }} {{ (subfooter_top_blocks_paddings) ? ' region--no-block-paddings' : '' }}{{ (subfooter_top_region_paddings) ? ' region--no-paddings' : '' }}">
          <div class="{{ subfooter_top_layout_container }}">
