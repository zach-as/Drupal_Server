      <div class="mt-mobile-menu-overlay" style="display:none">
        {% if page.mobile_menu_widget %}
          <div class="mt-mobile-menu-overlay-content region--shade-background container-fluid pt-4">
            <div class="row">
              <div class="col-12">
                {{ page.mobile_menu_widget }}
              </div>
            </div>
          </div>
        {% endif %}
        <button type="button" class="mt-dismiss-button mt-button region--shade-background {{ (mobile_menu_widget_sticky) ? 'mt-dismiss-button--sticky' }}">
          <span class="sr-only">{% trans %}Dismiss mobile overlay area{% endtrans %}</span><i class="fas fa-times"></i>
        </button>
      </div>

      {# Use the default search route defined in .theme file as the default form action. #}
      <form method="get" action="{{ url(default_search_route) }}">
        <div class="clearfix mt-mobile-menu-widget mt-container {{ (mobile_menu_widget_sticky) ? 'mt-sticky' }}">
          <div class="mt-search-icon">
            <i class="fas fa-search"></i>
          </div>
          <input type="text" name="keys" class="mt-input" placeholder="Search...">
          <div class="mt-clear-icon">
            <span class="sr-only">{% trans %}Clear keys input element{% endtrans %}</span><i class="fas fa-times-circle"></i>
          </div>
          <button type="submit" class="mt-submit-button mt-button">
            <span class="sr-only">{% trans %}Submit search{% endtrans %}</span><i class="fas fa-arrow-right"></i>
          </button>
          <button type="button" class="mt-menu-button mt-button">
            <span class="sr-only">{% trans %}Open mobile overlay area{% endtrans %}</span><i class="fas fa-bars"></i>
          </button>
        </div>
      </form>
    </div>
  {% endif %}
